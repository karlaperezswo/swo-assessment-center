{
  "name": "aws-assessment-generator",
  "version": "1.0.0",
  "description": "AWS Assessment Report Generator - Generates professional Word documents for AWS migration reports",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "npm run dev --workspace=frontend",
    "dev:backend": "npm run dev --workspace=backend",
    "build": "npm run build --workspaces",
    "start": "npm run start --workspace=backend"
  },
  "devDependencies": {
    "archiver": "^7.0.1",
    "concurrently": "^8.2.0"
  }
}
